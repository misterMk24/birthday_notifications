{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
